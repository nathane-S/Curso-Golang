package main

func qualidade(velocidade int) float64 {
	var taxa float64
	if velocidade == 0 {
		return taxa
	}
	if velocidade <= 4 {
		taxa = 100
		return taxa
	}
	if velocidade <= 8 {
		taxa = 90
		return taxa
	}
	if velocidade <= 10 {
		taxa = 77
		return taxa
	}
	return float64(0)
}

func producao(velocidade int) float64 {
	p := velocidade * 221
	return float64(p)
}

